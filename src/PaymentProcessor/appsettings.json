{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "EventBus": "amqp://localhost"
  },
  "EventBus": {
    "SubscriptionClientName": "PaymentProcessor"
  },
  "PaymentOptions": {
    "PaymentSucceeded": true,
    "UsePayPal": false,
    "PayPalClientId": "YOUR-PAYPAL-CLIENT-ID",
    "PayPalClientSecret": "YOUR-PAYPAL-CLIENT-SECRET",
    "PayPalEnvironment": "Sandbox",
    "CurrencyCode": "USD"
  }
}